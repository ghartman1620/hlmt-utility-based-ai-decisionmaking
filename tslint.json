{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file" :[false, 1],
        "trailing-comma": false,
        "max-line-length" : [true, 120],
        "no-unused-variable" : true,
        "no-console" : false,
        "no-debugger" : false,
        "ordered-imports" : false
    },
    "rulesDirectory": []
}
